<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="window.StartWindow">
    <children>
        <Pane layoutY="-30.0" prefHeight="600.0" prefWidth="809.0">
            <children>
                <Pane layoutX="3.0" layoutY="14.0" prefHeight="593.0" prefWidth="803.0">
                    <children>
                        <Button fx:id="createSysBtn" layoutX="334.0" layoutY="470.0" mnemonicParsing="false" onAction="#createSysBtnClick" prefHeight="50.0" prefWidth="135.0" text="Create new system" />
                        <Button fx:id="startSysBtn" layoutX="351.0" layoutY="247.0" mnemonicParsing="false" onAction="#startAccountingSystem" prefHeight="50.0" prefWidth="101.0" text="Start system" />
                        <Label fx:id="messageToUser" layoutX="14.0" layoutY="311.0" prefHeight="226.0" prefWidth="283.0" textAlignment="CENTER" />
                        <Label layoutX="353.0" layoutY="76.0" text="Accounting">
                            <font>
                                <Font name="Bell MT Bold" size="20.0" />
                            </font>
                        </Label>
                        <Button fx:id="loadBtn" layoutX="334.0" layoutY="342.0" mnemonicParsing="false" onAction="#loadBtnClick" prefHeight="50.0" prefWidth="135.0" text="Load database" />
                        <Label fx:id="systemInformation" contentDisplay="CENTER" layoutX="302.0" layoutY="119.0" prefHeight="116.0" prefWidth="206.0">
                            <font>
                                <Font name="Bell MT" size="12.0" />
                            </font>
                        </Label>
                    </children>
                </Pane>
            </children>
        </Pane>
    </children>
</AnchorPane>
